Vagrant.configure("2") do |config|
  config.vm.box = "hashicorp/bionic64"
  config.vm.provision :shell, path: "bootstrap.sh"
  config.vm.network :forwarded_port, guest: 80, host: 4567 # port forwarding to be able to access apache server from host into guest machine
  config.vbguest.auto_update = false  # Ignores version check for Guest Additions
end
